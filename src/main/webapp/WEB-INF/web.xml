<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  <servlet-name>sign</servlet-name>
  <servlet-class>studenta2withhtml.controller.SignUpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>sign</servlet-name>
  <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>studenta2withhtml.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>select</servlet-name>
  <servlet-class>studenta2withhtml.controller.SelectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>select</servlet-name>
  <url-pattern>/select</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>del</servlet-name>
  <servlet-class>studenta2withhtml.controller.DeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>del</servlet-name>
  <url-pattern>/delete</url-pattern>
  </servlet-mapping>
  
  
  
   <servlet>
  <servlet-name>up</servlet-name>
  <servlet-class>studenta2withhtml.controller.UpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>up</servlet-name>
  <url-pattern>/update</url-pattern>
  </servlet-mapping>
  
  
</web-app>
